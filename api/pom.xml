        <artifactId>kotlin-stdlib</artifactId>
        <version>1.3.70</version>
      </dependency>
    </dependencies>
  </dependencyManagement>


  <build>
    <finalName>${project.parent.artifactId}</finalName>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>build-helper-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>add-source</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>add-source</goal>
            </goals>
            <configuration>
              <sources>
                <source>src/codegen/java</source>
              </sources>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <!-- 代码生成器插件 -->
      <plugin>
        <groupId>org.jooq</groupId>
        <artifactId>jooq-codegen-maven</artifactId>
        <version>${jooq.version}</version>
        <configuration>
          <jdbc>
            <driver>com.mysql.cj.jdbc.Driver</driver>
            <url>jdbc:mysql://localhost:3306/bella_openapi?useSSL=false</url>
            <user>root</user>
            <password>123456</password>
          </jdbc>
          <generator>
            <database>
              <inputSchema>bella_openapi</inputSchema>
              <outputSchemaToDefault>true</outputSchemaToDefault>
              <includes>.*</includes>
              <unsignedTypes>false</unsignedTypes>
            </database>
            <strategy>
              <name>org.jooq.codegen.DefaultGeneratorStrategy</name>
              <matchers>
                <tables>
                  <table>
                    <daoClass>
                      <transform>PASCAL</transform>
                      <expression>$0_D_B_Dao</expression>
                    </daoClass>
                    <pojoClass>
                      <transform>PASCAL</transform>
                      <expression>$0_D_B</expression>
                    </pojoClass>
                    <pojoImplements>com.ke.bella.openapi.db.repo.Timed</pojoImplements>
                    <recordImplements>com.ke.bella.openapi.db.repo.Timed</recordImplements>
                    <expression>api_key_month_cost</expression>
                  </table>
                  <table>
                    <daoClass>
                      <transform>PASCAL</transform>
                      <expression>$0_D_B_Dao</expression>
                    </daoClass>
                    <pojoClass>
                      <transform>PASCAL</transform>
                      <expression>$0_D_B</expression>
                    </pojoClass>
                    <pojoImplements>com.ke.bella.openapi.db.repo.Operator</pojoImplements>
                    <recordImplements>com.ke.bella.openapi.db.repo.Operator</recordImplements>
                  </table>
                </tables>
              </matchers>
            </strategy>
            <generate>
              <deprecated>false</deprecated>
              <records>true</records>
              <pojos>true</pojos>
              <daos>false</daos>
            </generate>
            <target>
              <packageName>com.ke.bella.openapi</packageName>
              <directory>src/codegen/java</directory>
              <encoding>UTF-8</encoding>
            </target>
          </generator>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-deploy-plugin</artifactId>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-pmd-plugin</artifactId>
        <version>3.8</version>
        <configuration>
          <rulesets>
            <ruleset>rulesets/java/ali-comment.xml</ruleset>
            <ruleset>rulesets/java/ali-concurrent.xml</ruleset>
            <ruleset>rulesets/java/ali-constant.xml</ruleset>
            <ruleset>rulesets/java/ali-exception.xml</ruleset>
            <ruleset>rulesets/java/ali-flowcontrol.xml</ruleset>
            <ruleset>rulesets/java/ali-naming.xml</ruleset>
            <ruleset>rulesets/java/ali-oop.xml</ruleset>
            <ruleset>rulesets/java/ali-orm.xml</ruleset>
            <ruleset>rulesets/java/ali-other.xml</ruleset>
            <ruleset>rulesets/java/ali-set.xml</ruleset>
          </rulesets>
          <printFailingErrors>true</printFailingErrors>
        </configuration>
        <executions>
          <execution>
            <goals>
              <goal>check</goal>
            </goals>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>com.alibaba.p3c</groupId>
            <artifactId>p3c-pmd</artifactId>
            <version>1.3.6</version>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
  </build>
